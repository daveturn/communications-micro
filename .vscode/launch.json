  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceFolder}",
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--reload"],
      "jinja": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Shell",
      "type": "python",
      "pythonArgs": ["-i"],
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "env": {},
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
